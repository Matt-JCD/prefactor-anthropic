{
  "name": "@matt-dev-space/anthropic",
  "version": "0.1.0",
  "description": "Anthropic SDK integration for Prefactor observability",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "qa": "node scripts/run-local.mjs",
    "qa:quick": "node scripts/run-local.mjs --quick",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "prefactor",
    "observability",
    "tracing",
    "anthropic",
    "claude",
    "llm",
    "agent",
    "monitoring"
  ],
  "author": "Prefactor",
  "license": "MIT",
  "dependencies": {
    "@prefactor/core": "^0.2.9"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.70.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@types/node": "^25.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
